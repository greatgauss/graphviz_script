

digraph TetherMasterSM{
	size = "8.5, 11";
	fontname = "Microsoft YaHei";
	fontsize = 15;
	node [shape = record, fontname = "Microsoft YaHei", fontsize = 10];
	edge [fontname = "Microsoft YaHei", fontsize = 10];
	Initial -> TetherModeAlive [ label = "CMD_TETHER_MODE_REQUESTED" ];
